
@{
    ViewBag.Title = "RequestAddTemplete";
    Layout = "~/Views/Shared/_Layout_Add.cshtml";
}

<!-- Sweet Alert -->
<link href="../plugins/sweet-alert2/sweetalert2.min.css" rel="stylesheet" type="text/css">
<link href="../plugins/animate/animate.css" rel="stylesheet" type="text/css">

<!-- Page-Title -->
<div class="row">
    <div class="col-sm-12">
        <div class="page-title-box">
            <div class="float-right">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="javascript:void(0);">Dashboard</a></li>
                    <li class="breadcrumb-item"><a href="javascript:void(0);">Service Request</a></li>
                </ol>
            </div>
            <h4 class="page-title">Create Service Request Template</h4>
        </div><!--end page-title-box-->
    </div><!--end col-->
</div>


<div class="row">
    <div class="col-sm-12">
        <div class="card">
            <div class="card-body">
                <div class="form-group row">
                    <div class="col-md-6">
                        <div class="form-group row">
                            <label class="col-lg-2 col-form-label">Category </label>
                            <div class="col-lg-9">
                                <select class="select2 form-control mb-3 custom-select" id="ddlClient" style="width: 100%;">
                                    <option value="0"></option>
                                </select>
                            </div>
                        </div><!-- end col -->
                    </div>
                    <div class="col-md-6">
                        <div class="form-group row">
                            <label class="col-lg-2 col-form-label">Sub Category </label>
                            <div class="col-lg-9">
                                <select class="select2 form-control mb-3 custom-select" id="ddlClient" style="width: 100%;">
                                    <option value="0"></option>
                                </select>
                            </div>
                        </div><!-- end col -->
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-6">
                        <div class="form-group row">
                            <label class="col-lg-2 col-form-label">Title *</label>
                            <div class="col-lg-9">
                                <select class="select2 form-control mb-3 custom-select" id="ddlClient" style="width: 100%;">
                                    <option value="0"></option>
                                </select>
                            </div>
                        </div><!-- end col -->
                    </div>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-6">
                    <div class="form-group row">
                        <label class="col-lg-2 col-form-label">Request Description</label>
                        <div class="col-lg-9">
                            <select class="select2 form-control mb-3 custom-select" id="ddlClient" style="width: 100%;">
                                <option value="0"></option>
                            </select>
                        </div>
                    </div><!-- end col -->
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-6">
                    <div class="form-group row">
                        <label class="col-lg-2 col-form-label">Catalogue Description</label>
                        <div class="col-lg-9">
                            <select class="select2 form-control mb-3 custom-select" id="ddlClient" style="width: 100%;">
                                <option value="0"></option>
                            </select>
                        </div>
                    </div><!-- end col -->
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-6">
                    <div class="form-group row">
                        <label class="col-lg-2 col-form-label">Purchase Required </label>
                        <div class="col-lg-9">
                            <select class="select2 form-control mb-3 custom-select" id="ddlClient" style="width: 100%;">
                                <option value="0"></option>
                            </select>
                        </div>
                    </div><!-- end col -->
                </div>
                <div class="col-md-6">
                    <div class="form-group row">
                        <label class="col-lg-2 col-form-label">Approx. Fulfillment Time *</label>
                        <div class="col-lg-9">
                            <select class="select2 form-control mb-3 custom-select" id="ddlClient" style="width: 100%;">
                                <option value="0"></option>
                            </select>
                        </div>
                    </div><!-- end col -->
                </div>
                </div>
                <div class="row">
                    <div class="col-md-4 text-lg-right">
                        <button class="btn btn-gradient-primary" id="btnSubmit" type="submit">Next</button>
                    </div>
                </div>
            </div><!--end card-body-->
        </div>
        <!--end card--> <!--end card-body--> <!--end card-->
    </div><!--end col-->
</div>



<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/jquery.min.js"></script>
